<tr>
  <td>
    <%= user.id %>
  </td>
  <td>
    <%= user.email %>
  </td>
  <td>
    <%= user.age || 'Uknown' %>
  </td>
  <td>
    <%= user.gender %>
  </td>
  <td>
    <%= user.interests.map(&:name).join ', ' %>
  </td>
  <td>
    <%= link_to edit_admin_user_path(user), class: 'btn btn-primary btn-xs' do %>
      Edit
      &nbsp;
      <i class="fa fa-pencil"></i>
    <% end %>
  </td>
</tr>
