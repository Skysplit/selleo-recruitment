<table class="table table-bordered table-responsive table-striped">
  <thead>
    <tr>
      <th>#</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td>
        <%= user.id %>
      </td>
      <td>
        <%= user.email %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
